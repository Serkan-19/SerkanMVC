@model List<OrderGetDtoItem>


<br />
<br />
<br />
<br />
<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">ORDERS</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Emir No</th>
                        <th>Gemi Adı</th>
                        <th>Gemi Çıkış Tarihi</th>
                        <th>Şirket Adı</th>
                        <th>İşçinin Adı</th>
                        <th>İşçinin Soyadı</th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>Emir No</th>
                        <th>Gemi Adı</th>
                        <th>Gemi Çıkış Tarihi</th>
                        <th>Şirket Adı</th>
                        <th>İşçinin Adı</th>
                        <th>İşçinin Soyadı</th>
                    </tr>
                </tfoot>
                <tbody>
                    @foreach (var order in Model)
                    {
                       
                    <tr>
                        <td>@order.emirNo</td>
                        <td>@order.gemiAdi</td>
                        <td>@order.gemiCikisTarihi</td>
                        <td>@order.sirketAdi</td>
                        <td>@order.iscininAdi</td>
                        <td>@order.iscininSoyadi</td>
                    </tr>
                    }
                   
                
