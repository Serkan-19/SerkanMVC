<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Serkan_MVC</title>
   
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Serkan_MVC.styles.css" asp-append-version="true" />
    <link href="~/Document/vendor/fontawesome-free/css/all.min.css" rel="stylesheet" type="text/css">
    <link href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
          rel="stylesheet">
    <link href="~/Document/css/sb-admin-2.min.css" rel="stylesheet">
   
</head>
<body>
    <header>
        
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>




    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2023 - Serkan_MVC - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    <script src="~/Document/vendor/jquery/jquery.min.js"></script>
    <script src="~/Document/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Document/vendor/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/Document/js/sb-admin-2.min.js"></script>
    <script src="~/Document/vendor/jquery/jquery.min.js"></script>
    <script src="~/Document//bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/Document/vendor/jquery-easing/jquery.easing.min.js"></script>
    <script src="~/Document/js/sb-admin-2.min.js"></script>
    <script src="~/Document/vendor/chart.js/Chart.min.js"></script>
    <script src="~/Document/js/demo/chart-area-demo.js"></script>
    <script src="~/Document/js/demo/chart-pie-demo.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
